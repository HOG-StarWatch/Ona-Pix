<UserControl x:Class="Ona_Pix.Ctrls.ToggleSwitch"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             mc:Ignorable="d"
             Height="16.5" Width="27.5">
    <Grid>
        <Rectangle x:Name="BackRec" Fill="#FFC8C8C8" Height="16.5" Width="27.5" RadiusY="8.25" RadiusX="8.25" MouseDown="BackRec_MouseDown" />
        <Ellipse x:Name="ForeDot" Fill="#FF2196F3" Height="14" Width="14" MouseDown="ForeDot_MouseDown" Margin="-11,0,0,0" />
    </Grid>
</UserControl>